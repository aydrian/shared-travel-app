{
  "name": "shared-travel-app",
  "version": "0.1.0",
  "private": true,
  "description": "RBAC and ReBAC authorization demo using Oso Cloud",
  "author": "Aydrian Howard <howdy@itsaydrian.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/aydrian/ai-driven-authz-demo.git"
  },
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy --minify",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "wrangler d1 migrations apply shared-travel-app-d1",
    "db:migrate-remote": "wrangler d1 migrations apply shared-travel-app-d1 --remote",
    "db:studio": "drizzle-kit studio",
    "db:seed": "bun run src/db/seeds/index.ts",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:clean": "docker-compose down -v",
    "oso:typegen": "oso-cloud generate-types typescript main.polar > src/lib/polarTypes.d.ts --include facts,queries,resources",
    "oso:validate": "oso-cloud validate main.polar",
    "oso:test": "oso-cloud test main.polar",
    "test": "vitest",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@better-auth-kit/tests": "^0.2.0",
    "@hono/zod-validator": "^0.5.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "better-auth": "^1.2.8",
    "drizzle-orm": "^0.43.1",
    "hono": "^4.7.10",
    "oso-cloud": "^2.4.7",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@cloudflare/vitest-pool-workers": "^0.8.30",
    "drizzle-kit": "^0.31.1",
    "typescript": "^5.8.3",
    "vitest": "^3.1.3",
    "wrangler": "^4.15.2"
  }
}